const int N;

!E [ F obstacle&closed&!open ]

filter(avg, R{"step"}=? [ F "fail" ], "s0")

filter(avg, P=? [ F<=N "fail" ], "s0")

filter(avg, R{"step"}=? [ F obstacle&partially&!open ], "s0")

filter(avg, R{"step"}=? [ F pickup&open ], "s0")

filter(avg, R{"step"}=? [ F !getonandoff&open ], "s0")

filter(avg, P=? [ F<=N obstacle&partially&!open ], "s0")

filter(avg, P=? [ F<=N pickup&open ], "s0")

filter(avg, P=? [ F<=N !getonandoff&open ], "s0")

filter(avg, R{"step"}=? [ F pickup&opened&!close ], "s0")

filter(avg, R{"step"}=? [ F obstacle&close ], "s0")

filter(avg, P=? [ F<=N pickup&opened&!close ], "s0")

filter(avg, P=? [ F<=N obstacle&close ], "s0")

